<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lyhux.mall.mapper.UserMapper" >
    <resultMap id="userVOMapper" type="com.lyhux.mall.model.UserVO">
    </resultMap>
    <select id="selectUsers" resultMap="userVOMapper">
        SELECT id, username  from admin LIMIT 10
    </select>

    <select id="selectUser" parameterType="int" resultMap="userVOMapper">
        SELECT id, username as name from admin WHERE id=#{id} LIMIT 1
    </select>


</mapper>